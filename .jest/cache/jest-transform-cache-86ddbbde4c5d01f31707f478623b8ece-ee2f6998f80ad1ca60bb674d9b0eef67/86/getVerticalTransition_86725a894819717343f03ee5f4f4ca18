exports.getVerticalTransition = void 0;

var _Types = require("../Types");

var getVerticalTransition = function getVerticalTransition(transitionInfo) {
  if (!transitionInfo || transitionInfo.metrics === undefined) {
    return {};
  }

  var start = transitionInfo.start,
      end = transitionInfo.end,
      dimensions = transitionInfo.dimensions;
  var startPosition = 0;
  var endPosition = 0;

  if (transitionInfo.direction === _Types.RouteDirection.from) {
    startPosition = 0;
    endPosition = -(dimensions.height + 25);
  } else if (transitionInfo.direction === _Types.RouteDirection.to) {
    startPosition = dimensions.height + 25;
    endPosition = 0;
  }

  var transitionProgress = transitionInfo.progress.interpolate({
    inputRange: [0, start, end, 1],
    outputRange: [startPosition, startPosition, endPosition, endPosition]
  });
  return {
    transform: [{
      translateY: transitionProgress
    }]
  };
};

exports.getVerticalTransition = getVerticalTransition;
