var _Types = require("../Types");

var getLeftTransition = function getLeftTransition(transitionSpecification) {
  if (!transitionSpecification || transitionSpecification.metrics === undefined) {
    return {};
  }

  var start = transitionSpecification.start,
      end = transitionSpecification.end,
      boundingbox = transitionSpecification.boundingbox;
  var x = boundingbox.x,
      width = boundingbox.width;
  var distanceValue = -(width + x + 25);
  var startValue = 0;
  var endValue = distanceValue;

  if (transitionSpecification.direction === _Types.RouteDirection.to) {
    startValue = distanceValue;
    endValue = 0;
  }

  var progress = transitionSpecification.progress.interpolate({
    inputRange: [0, start, end, 1],
    outputRange: [startValue, startValue, endValue, endValue]
  });
  return {
    transform: [{
      translateX: progress
